{
  "name": "session-notes-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:setup": "node database/migration.js",
    "db:setup-with-data": "node database/migration.js --sample-data",
    "db:reset": "node database/migration.js --sample-data",
    "db:remove-billing": "psql $DATABASE_URL -f database/remove_billing.sql",
    "docker:up": "docker-compose up -d && node scripts/print-info.js",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose up -d --build && node scripts/print-info.js"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "jsonwebtoken": "^9.0.2",
    "next": "15.5.2",
    "openai": "^5.19.1",
    "pg": "^8.11.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
